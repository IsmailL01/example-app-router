#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ismail/Downloads/next-intl-main/node_modules/.pnpm/next@14.2.4_@babel+core@7.24.8_@playwright+test@1.44.1_react-dom@18.3.1_react@18.3.1/node_modules/next/dist/bin/node_modules:/Users/ismail/Downloads/next-intl-main/node_modules/.pnpm/next@14.2.4_@babel+core@7.24.8_@playwright+test@1.44.1_react-dom@18.3.1_react@18.3.1/node_modules/next/dist/node_modules:/Users/ismail/Downloads/next-intl-main/node_modules/.pnpm/next@14.2.4_@babel+core@7.24.8_@playwright+test@1.44.1_react-dom@18.3.1_react@18.3.1/node_modules/next/node_modules:/Users/ismail/Downloads/next-intl-main/node_modules/.pnpm/next@14.2.4_@babel+core@7.24.8_@playwright+test@1.44.1_react-dom@18.3.1_react@18.3.1/node_modules:/Users/ismail/Downloads/next-intl-main/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ismail/Downloads/next-intl-main/node_modules/.pnpm/next@14.2.4_@babel+core@7.24.8_@playwright+test@1.44.1_react-dom@18.3.1_react@18.3.1/node_modules/next/dist/bin/node_modules:/Users/ismail/Downloads/next-intl-main/node_modules/.pnpm/next@14.2.4_@babel+core@7.24.8_@playwright+test@1.44.1_react-dom@18.3.1_react@18.3.1/node_modules/next/dist/node_modules:/Users/ismail/Downloads/next-intl-main/node_modules/.pnpm/next@14.2.4_@babel+core@7.24.8_@playwright+test@1.44.1_react-dom@18.3.1_react@18.3.1/node_modules/next/node_modules:/Users/ismail/Downloads/next-intl-main/node_modules/.pnpm/next@14.2.4_@babel+core@7.24.8_@playwright+test@1.44.1_react-dom@18.3.1_react@18.3.1/node_modules:/Users/ismail/Downloads/next-intl-main/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../next/dist/bin/next" "$@"
else
  exec node  "$basedir/../next/dist/bin/next" "$@"
fi
